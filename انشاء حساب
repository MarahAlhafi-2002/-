    <input type="number" id="age" name="age" min="18" max="60" required oninput="checkAgeValidity()">
    <div id="ageError" style="color: red;"></div>
</div>

<script>
function checkAgeValidity() {
    var ageInput = document.getElementById('age');
    var ageError = document.getElementById('ageError');
    
    if (ageInput.value < 18 || ageInput.value > 60) {
        ageError.textContent = ' ';
        ageInput.setCustomValidity('يجب أن يكون عمرك ما بين 18 و 60 سنة');
    } else {
        ageError.textContent = '';
        ageInput.setCustomValidity('');
    }
}
</script>

